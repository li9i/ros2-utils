<launch>
  <arg name="frames_filename"   default="frames.yaml"/>
  <node pkg="static_tf_republisher" exec="static_tf_republisher_node" name="static_tf_republisher_node" output="screen">
    <param name="frames_yaml"   value="$(find-pkg-share static_tf_republisher)/config/$(var frames_filename)"/>
    <param name="publish_rate"  value="2.0"/>
  </node>
</launch>
